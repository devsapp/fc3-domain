{
    "transformIgnorePatterns": [
      "/example/*.*"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest" 
    },
    "testRegex": "(/__tests__/(ut|it)/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$", 
    "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "node"
    ],
    "collectCoverage": true,
    "coverageReporters": ["html", "text"]
  }
  